public class App {
    public void printArgs(String[] args) {
	for (String arg : args) {
	    System.out.println(arg);
	}
    }

    public String[] toUpperCase(String[] args) {
	int i = 0;

	for (String arg : args) {
	    args[i] = args[i].toUpperCase();
	    i++;
	}
	return(args);
    }

    public String[] removeChar(String[] args, char c) {
	int i = 0;

	for (String arg : args) {
	    args[i] = arg.replace(c, '\0');
	    i++;
	}
	return(args);
    }

    public static void main(String[] args) {
	App app = new App();

	String[] tab = new String[] {"Hello", "world","!"};
	app.printArgs(tab);
	app.toUpperCase(tab);
	app.removeChar(tab, 'L');
	app.printArgs(tab);
    }
}